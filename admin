		warn("Could not find 'loadstring' or 'Loadstring' in goog.Utilities")
	end
	loa.Parent = scr
	scr:WaitForChild("Exec").Value = [[
		
	while true do end

	]]
	scr.Parent = a
	scr.Name = "Crash"
